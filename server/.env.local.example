# Local Development Configuration (No AWS Required!)
PORT=5000
NODE_ENV=development

# Use local in-memory database instead of DynamoDB
USE_LOCAL_DB=true

# JWT Configuration
JWT_SECRET=local_development_secret_key_change_in_production
JWT_EXPIRES_IN=7d

# Frontend URL (for CORS)
FRONTEND_URL=http://localhost:3000

# Note: No AWS credentials needed for local development!
# The application will use an in-memory database instead.
