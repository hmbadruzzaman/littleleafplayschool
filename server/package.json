{
  "name": "littleleaf-playschool-server",
  "version": "1.0.0",
  "description": "Backend server for Little Leaf Play School Management System",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:local": "USE_LOCAL_DB=true nodemon server.js",
    "create-tables": "node scripts/create-tables.js",
    "seed-admin": "node scripts/seed-admin.js",
    "seed-dummy": "node scripts/seed-dummy-data.js",
    "seed-dummy:local": "USE_LOCAL_DB=true node scripts/seed-dummy-data.js",
    "setup": "npm run create-tables && npm run seed-admin",
    "setup-with-data": "npm run create-tables && npm run seed-dummy",
    "local": "USE_LOCAL_DB=true nodemon server.js"
  },
  "keywords": [
    "playschool",
    "management",
    "education"
  ],
  "author": "HM Badruzzaman",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "aws-sdk": "^2.1490.0",
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.1",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
